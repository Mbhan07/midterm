#include <iostream>
#include <cstring>

using namespace std;

int main(){

  bool givingInput = true;

  int one;
  int two;
  int three;
  int four;
  int five;
  int six;
  int seven;
  int num;

  while (givingInput){

    cout << "Enter a number: ";
    cin >> num;
    count++;
    for (int i = 0; i < 7; i++){
      if(count[i] == count[0]){
	num = one;
      }else if(count[i] == count[1]){
	num = two;
      }else if(count[i] == count[2]){
        num = three;
      }else if(count[i] == count[3]){
        num = four;
      }else if(count[i] == count[4]){
        num = five;
      }else if(count[i] == count[5]){
        num = six;
      }else if(count[i] == count[6]){
        num = seven;
      }
    }

    exit(0);
  }
  

  int product = one * two * three * four * five * six * seven;
  cout << product << endl;
  return 0;

}
